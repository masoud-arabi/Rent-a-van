<ol>
  <% @bookings.each do |booking| %>
    <div>
      <% current_user%>
      <a><%= link_to booking.user.email, booking_path(booking) %></a>
      <li>From <%= booking.start_date %> --- To <%= booking.end_date %><%= booking.approved %>
      <p><%= booking.van.title %></p></li>
    </div>
  <% end %>
</ol>


