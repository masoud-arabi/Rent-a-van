<h1>title : <%= @van.title %></h1>
<h1>price : <%= @van.price %></h1>
<h1>description : <%= @van.description %></h1>
<h1>address : <%= @van.address %></h1>

<p><%= link_to "Edit", edit_van_path, class: "btn btn-outline-success" %></p>
<p><%= link_to "Delete", new_van_path, method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-outline-success" %></p>
<p><%= link_to "back to see all vans", van_path(params[:id]) %></p>

